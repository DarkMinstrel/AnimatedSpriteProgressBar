<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="aspb">
        <attr name="sprites" format="reference"/>
        <attr name="duration" format="integer"/>
        <attr name="rotations" format="integer"/>
        <attr name="adapter" format="enum">
            <enum name="trapeze" value="0"/>
            <enum name="triangle" value="1"/>
            <enum name="square" value="2"/>
        </attr>
        <attr name="interpolator" format="enum">
            <enum name="accelerate_decelerate" value="0"/>
            <enum name="linear" value="1"/>
        </attr>
    </declare-styleable>
</resources>